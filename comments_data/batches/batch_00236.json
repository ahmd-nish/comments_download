        "vote_sum": 3,
        "vote_count": 3,
        "official": false,
        "html_url": "https://feedback.minecraft.net/hc/en-us/community/posts/24233164595853-Boggy-Bone/comments/24233218713997",
        "created_at": "2024-02-18T23:33:34Z",
        "updated_at": "2024-02-29T17:01:04Z",
        "url": "https://minecraftfeedback.zendesk.com/api/v2/help_center/community/posts/24233164595853/comments/24233218713997.json",
