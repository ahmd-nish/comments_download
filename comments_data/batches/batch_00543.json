        "non_author_editor_id": null,
        "non_author_updated_at": null
      },
      {
        "id": 360013672372,
        "body": "<p>Shields should also be able to be wielded at once</p>",
        "author_id": 406280981491,
        "vote_sum": 2,
        "vote_count": 2,
        "official": false,
        "html_url": "https://feedback.minecraft.net/hc/en-us/community/posts/360074943111-Swords-in-offhands-should-be-able-to-be-used-as-regular-swords/comments/360013672372",
        "created_at": "2020-12-01T02:40:51Z",
