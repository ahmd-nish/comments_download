      {
        "id": 38195514233997,
        "body": "<p>any criticism would be appreciated</p>",
        "author_id": 38195450815757,
        "vote_sum": 0,
        "vote_count": 0,
        "official": false,
        "html_url": "https://feedback.minecraft.net/hc/en-us/community/posts/38195540196621-Charging-battery/comments/38195514233997",
        "created_at": "2025-07-20T18:57:48Z",
        "updated_at": "2025-07-20T18:57:48Z",
