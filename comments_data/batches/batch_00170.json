        "vote_sum": 1,
        "vote_count": 1,
        "official": false,
        "html_url": "https://feedback.minecraft.net/hc/en-us/community/posts/5852488837517-Droem-temporary-dimension/comments/5874723952781",
        "created_at": "2022-04-28T22:19:03Z",
