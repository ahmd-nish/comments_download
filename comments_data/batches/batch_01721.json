      {
        "id": 360013891552,
        "body": "<p>Hoppers would then become obsolete</p>",
        "author_id": 406280981491,
        "vote_sum": 1,
        "vote_count": 1,
        "official": false,
        "html_url": "https://feedback.minecraft.net/hc/en-us/community/posts/360075814912-Pipes/comments/360013891552",
        "created_at": "2020-12-21T01:17:29Z",
        "updated_at": "2020-12-23T18:58:05Z",
        "url": "https://minecraftfeedback.zendesk.com/api/v2/help_center/community/posts/360075814912/comments/360013891552.json",
