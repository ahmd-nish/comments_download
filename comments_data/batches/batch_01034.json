        "author_id": 32141730230157,
        "vote_sum": 2,
        "vote_count": 2,
        "official": false,
        "html_url": "https://feedback.minecraft.net/hc/en-us/community/posts/32144409225997-New-weapon-Anchor/comments/32166952379277",
