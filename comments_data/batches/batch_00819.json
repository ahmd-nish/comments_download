      {
        "id": 360013001711,
        "body": "<p>They wouldn’t be blind they would be more sensitive to light</p>",
        "author_id": 389351405251,
        "vote_sum": 0,
        "vote_count": 0,
        "official": false,
        "html_url": "https://feedback.minecraft.net/hc/en-us/community/posts/360073513131-warden-texture-change/comments/360013001711",
        "created_at": "2020-10-06T12:19:50Z",
